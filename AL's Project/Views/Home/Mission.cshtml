
@{
    ViewBag.Title = "Mission";
}

<!-- TESTIMONIAL -->
<section class="site-testimonial section-grey" id="site-testimonial" ng-controller="missionController">
    <div class="container-fluid wide">
        <div class="row">
            <div class="col-sm-12">
                <!-- H1 HEADING -->
                <h1>{{mission.Title}}</h1>
                <div class="col-xs-12 col-sm-12 col-md-3 col-lg-3">
                </div>
                <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6">
                    <div class="main-heading-text">
                        <p ng-bind-html="mission.SubTitle | unsafe">
                        </p>
                    </div>
                </div>

            </div>
        </div>
    </div>
</section>
<script src="~/Content/Website/angular/mission/MissionModel.js"></script>
<script src="~/Content/Website/angular/mission/MissionService.js"></script>
<script src="~/Content/Website/angular/mission/MissionController.js"></script>
